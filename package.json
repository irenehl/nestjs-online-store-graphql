{
  "name": "nestjs-nerdery-challenge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@aws-sdk/client-s3": "^3.352.0",
    "@aws-sdk/s3-request-presigner": "^3.362.0",
    "@nestjs/apollo": "^12.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.7.1",
    "prisma": "^4.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.350.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-sdk": "^2.1397.0",
    "bcrypt": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "jest-mock-extended": "^3.0.4",
    "joi": "^17.9.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.(t|j)s",
      "!**/*.arg.(t|j)s",
      "!**/*.input.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/*.guard.(t|j)s",
      "!**/*.strategy.(t|j)s",
      "!**/*.decorator.(t|j)s",
      "!**/*.validator.(t|j)s",
      "!**/*.entity.(t|j)s",
      "!**/*.pipe.(t|j)s",
      "!**/*.type.(t|j)s",
      "!**/*.html.(t|j)s",
      "!**/*.enum.(t|j)s",
      "!**/*.mock.(t|j)s",
      "!**/*main.(t|j)s",
      "!**/config/*"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@config/(.*)": "<rootDir>/config/$1",
      "@common/(.*)": "<rootDir>/common/$1",
      "@mocks/(.*)": "<rootDir>/mocks/$1",
      "@user/(.*)": "<rootDir>/user/$1",
      "@auth/(.*)": "<rootDir>/auth/$1",
      "@product/(.*)": "<rootDir>/product/$1",
      "@cart/(.*)": "<rootDir>/cart/$1",
      "@aws/(.*)": "<rootDir>/aws/$1",
      "@category/(.*)": "<rootDir>/category/$1",
      "@pipes/(.*)": "<rootDir>/pipes/$1",
      "@mail/(.*)": "<rootDir>/mail/$1",
      "@validators/(.*)": "<rootDir>/validators/$1"
    },
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  }
}
